struct Point {
    pub x: i32,
    pub y: i32
}

struct Color(u8, u8, u8);

struct Container<T> {
    value: T
}

fn main() {
    // Test struct literal creation
    let p1 = Point { x: 10, y: 20 };
    
    // Test field access
    let x_val = p1.x;
    let y_val = p1.y;
    
    // Test struct update syntax
    let p2 = Point { x: 5, ..p1 };
    
    // Test tuple struct creation
    let red = Color(255, 0, 0);
    
    // Test generic struct
    let container = Container { value: 42 };
    
    println!("Point 1: ({}, {})", p1.x, p1.y);
    println!("Point 2: ({}, {})", p2.x, p2.y);
    println!("Values: x={}, y={}", x_val, y_val);
}